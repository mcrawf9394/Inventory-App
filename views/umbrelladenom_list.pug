extends layout 

block content 
    h1= title 
    if umbrelladenom_list.length 
        ul 
            each umbrelladenom in umbrelladenom_list 
                li 
                    a(href=umbrelladenom.url) #{umbrelladenom.name}
        a(href="/catalog/branch/add") Add Branch 
    else 
        p There are no branches.
        a(href="/catalog/branch/add") Add Branch